// A simple example showing some input
println("Hi handsome, what is your name?")
name = input()
println("And how old are you?")

(age, errMsg) = input().parseInt()
if errMsg != nil {
    println("Thats not a Integer my dear")
    exit(1)
}

if age < 5 {
    println("Aren't you adorable, already programming like a real hacker")
} else {
    println("Nice to meet you " + name + "!")
}